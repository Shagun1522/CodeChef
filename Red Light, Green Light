#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t!=0)
	{
     int n,k,i,count=0;
     cin>>n>>k;
     int arr[n];
     for(i=0;i<n;i++)
     {
         cin>>arr[i];
         if(arr[i]>k)
         {
          count++;   
         }
     }
     cout<<count<<endl;
     t--;
	}
	
	return 0;
}
